package Main;

import java.util.ArrayList;
import java.util.List;
public class Main {

	public static void main(String[] args) {
		
		
		List<String> list = new ArrayList<>();

		list.add("Maria Aparecida");
		list.add("Aghata");
		list.add("Marcos");
		list.add("Joao victor");
		
		for (String x : list) {
			System.out.println(x);
		}
		
		
		
	}

}
